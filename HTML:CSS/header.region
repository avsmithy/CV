<!doctype html>

<html class="no-js">

  <head>
    
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1">

    <!--INCLUDE SQS SCRIPTS, META TAGS AND USER CONTENT FROM THE CODE INJECTION TAB-->
    
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400italic,600italic,400,600' rel='stylesheet' type='text/css'>

    {squarespace-headers}

    <squarespace:script src="sqsp.js" combo="true" />
    <squarespace:script src="jquery-1.9.1.min.js" combo="true" />
    <squarespace:script src="jquery.royalslider.custom.min.js" combo="true" />
    <squarespace:script src="responsive-tables.js" combo="true" />
    <squarespace:script src="main.js" combo="true" />
  
  </head>

  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    
    <!--HEADER-->

    <header id="header">

      <div id="headerMain">
      
        <div class="left responsive">
        <!--SITE TITLE OR LOGO-->

        {.section website}

          <h1 data-content-field="site-title">
            <a href="/">
              {.section logoImageUrl}
               <img src="{logoImageUrl}?format=original" alt="{siteTitle}" />
              {.or}
                {siteTitle}
              {.end}
            </a>
          </h1>
          
          <!--SITE TAGLINE-->

         <!-- {.section siteTagLine}<div id="tagline">{@}</div>{.end} -->

        {.end}
        </div>
        
        {.section websiteSettings}<div class="right responsive">
          <a href="/quote"><div class="header-quote quote-button responsive">FREE Quote!</div></a>
          <div class="header-phone responsive">{contactPhoneNumber}</div>
          <!--<a href="mailto:{contactEmail|htmlattr}">{contactEmail}</a>-->
        </div>{.end}
        
      <div class="clear"></div>
      </div>

      <!--MAIN NAVIGATION-->

      <squarespace:navigation navigationId="mainNav" template="navigation" />
      <div id="left-corner"></div>
      <div id="right-corner"></div>

    </header>